{
  "name": "enton",
  "version": "0.0.1-beta.12",
  "description": "Web framework for building Node.js application",
  "main": "./lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "enton": "./bin/enton.js"
  },
  "scripts": {
    "lint": "eslint src",
    "build": "babel src -d lib -s inline",
    "precommit": "lint-staged",
    "test": "jest -u",
    "coverage": "yarn test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinityjs/infinity.git"
  },
  "keywords": [
    "node",
    "express",
    "javascript",
    "es6"
  ],
  "author": "Sravan Kalluri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infinityjs/infinity/issues"
  },
  "homepage": "https://github.com/infinityjs/infinity#readme",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cross-spawn": "^6.0.5",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "lodash": "^4.17.11",
    "rc": "^1.2.8",
    "read-pkg": "^4.0.1",
    "yargs": "^13.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@commitlint/cli": "^7.3.1",
    "@commitlint/config-conventional": "^7.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "commitlint": "^7.3.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "engines": {
    "node": ">=11.6.0",
    "yarn": ">=1.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "directories": {
    "lib": "lib"
  }
}
