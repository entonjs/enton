{
  "name": "infinity",
  "version": "0.0.0",
  "description": "Web framework for building Node.js application",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinityjs/infinity.git"
  },
  "keywords": [
    "node",
    "express",
    "javascript",
    "es6"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infinityjs/infinity/issues"
  },
  "homepage": "https://github.com/infinityjs/infinity#readme",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.3.1",
    "@commitlint/config-conventional": "^7.3.1",
    "commitlint": "^7.3.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "engines": {
    "node": ">=11.6.0",
    "yarn": ">=1.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
